generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String

  @@index([name])
}

model Post {
  id     Int    @id @default(autoincrement())
  title  String
  body   String
  likes  Int
  userId Int

  @@index([title])
  @@index([userId])
}
